#include <stdio.h>

int main(void) {
    double R2 = 4.0;   // example: ohms
    double R3 = 6.0;   // example: ohms
    double RF = 8.0;   // example: ohms
    double V  = 12.0;  // example: volts

    double R23 = R2 + R3;                       // series
    double Req;

    if (R23 + RF == 0.0) {
        printf("Error: division by zero in parallel calculation.\n");
        return 1;
    }
    Req = (R23 * RF) / (R23 + RF);              // parallel

    double I_branch = V / Req;                  // total current through that parallel combination
    double I_F = V / RF;                        // current through RF
    double I_23 = V / R23;                      // current through series pair (same through R2 and R3)
    double V_R2 = I_23 * R2;
    double V_R3 = I_23 * R3;

    printf("R2 = %.2f ohm, R3 = %.2f ohm, RF = %.2f ohm\n", R2, R3, RF);
    printf("R23 (series) = %.2f ohm\n", R23);
    printf("Req (R23 // RF) = %.2f ohm\n\n", Req);

    printf("Supply V = %.2f V\n", V);
    printf("I_branch (through parallel combo) = %.2f A\n", I_branch);
    printf("I_F = %.2f A\n", I_F);
    printf("I_23 = %.2f A\n", I_23);
    printf("Voltage across R2 = %.2f V\n", V_R2);
    printf("Voltage across R3 = %.2f V\n", V_R3);

    return 0;
}
