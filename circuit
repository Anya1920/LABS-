#include <stdio.h>

int main() {
    // Given resistor and voltage values (you can later replace with user input)
    float R1 = 3.0, R2 = 6.0, R3 = 2.0, RF = 4.0; 
    float V = 12.0; // Supply voltage

    // Declare variables
    float R23, R_eq_parallel, R_total, I_total, V_R1, V_parallel;

    // Calculate R23 (R2 and R3 in parallel)
    R23 = (R2 * R3) / (R2 + R3);

    // Calculate equivalent parallel of R23 with RF
    R_eq_parallel = (R23 * RF) / (R23 + RF);

    // Total resistance (R1 in series with the parallel network)
    R_total = R1 + R_eq_parallel;

    // Total current from the source (Ohmâ€™s Law)
    I_total = V / R_total;

    // Voltage drops
    V_R1 = I_total * R1;
    V_parallel = V - V_R1; // Remaining voltage across the parallel network

    // Display results (2 decimal places)
    printf("=== Circuit Calculation Results ===\n");
    printf("R1 = %.2f ohms\n", R1);
    printf("R2 = %.2f ohms\n", R2);
    printf("R3 = %.2f ohms\n", R3);
    printf("RF = %.2f ohms\n", RF);
    printf("Supply Voltage (V) = %.2f V\n\n", V);

    printf("R23 (R2 || R3) = %.2f ohms\n", R23);
    printf("R_eq_parallel (R23 || RF) = %.2f ohms\n", R_eq_parallel);
    printf("Total Resistance (R_total) = %.2f ohms\n", R_total);
    printf("Total Current (I_total) = %.2f A\n", I_total);
    printf("Voltage across R1 = %.2f V\n", V_R1);
    printf("Voltage across parallel part (R23 || RF) = %.2f V\n", V_parallel);

    return 0;
}